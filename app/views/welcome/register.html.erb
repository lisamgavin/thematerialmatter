<h1>Register</h1>

  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>

  <div class="field">
    <%= f.label :password %><br />
    <%= f.text_area :password %>
  </div>

  <div class="actions">
    <%= f.submit "Create User"%>
  </div>

<div class="actions">
  <%= submit_tag 'Register' %>
  <%= button_to 'Register', method: :register %>
</div>
